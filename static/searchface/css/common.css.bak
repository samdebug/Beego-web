.enter-search {
	height: 34px;
	line-height: 34px;
	width: 400px;
	box-sizing: border-box;
	border: 1px solid #7bf5fe;
	background: rgba(27,53,56,.9);
	color: #7bf5fe;
	font-size: 14px;
	border-radius: 2px;
	text-align: center;
	position: relative;
	opacity: .6;
	float: right;
	display: inline-block
}

.enter-search.focus {
	opacity: 1
}

.enter-search span:first-child {
	width: 25px;
	height: 25px;
	background: url(../img/search.png) center no-repeat;
	background-size: 80% 80%;
	position: absolute;
	left: 10px;
	top: 4px;
	transition: .1s all ease;
	-webkit-transition: .5s all ease
}

.enter-search span:last-child {
	position: absolute;
	width: 60px;
	height: 20px;
	line-height: 20px;
	border: 1px solid #2d6267;
	background: #2d6267;
	border-radius: 2px;
	right: 5px;
	top: 6px;
	cursor: pointer
}

.enter-search span:last-child:hover {
	border: 1px solid #7bf5fe
}

.enter-search input {
	position: absolute;
	left: 40px;
	top: 3px;
	display: inline-block;
	width: 280px;
	line-height: 28px;
	height: 28px;
	border: none;
	color: #7bf5fe;
	transition: .1s all ease;
	-webkit-transition: .5s all ease
}

.click-search {
	display: inline-block;
	height: 34px;
	line-height: 34px;
	width: 200px;
	font-size: 14px;
	box-sizing: border-box;
	border: 1px solid #7bf5fe;
	color: #7bf5fe;
	border-radius: 2px;
	text-align: center;
	position: relative;
	opacity: .6
}

.click-search.focus {
	opacity: 1
}

.click-search span:first-child {
	width: 25px;
	height: 25px;
	background: url(../img/search.png);
	background-size: 100% 100%;
	position: absolute;
	left: 45px;
	top: 4px;
	transition: .1s all ease;
	-webkit-transition: .5s all ease
}

.click-search span:last-child {
	width: 20px;
	height: 20px;
	background: url(../img/close.png) center no-repeat;
	background-size: 80% 80%;
	position: absolute;
	padding: 3px;
	box-sizing: border-box;
	border: 1px solid #397b81;
	border-radius: 2px;
	right: 5px;
	top: 6px;
	cursor: pointer;
	display: none
}

.click-search span:last-child:hover {
	border: 1px solid #7bf5fe
}

.click-search.focus span:first-child {
	left: 20px
}

.click-search.focus span:last-child {
	display: inline-block
}

.click-search input {
	position: absolute;
	left: 70px;
	top: 0;
	display: inline-block;
	width: 120px!important;
	line-height: 30px;
	height: 30px;
	border: none!important;
	box-shadow: none!important;
	color: #7bf5fe;
	transition: .1s all ease;
	-webkit-transition: .5s all ease
}

.click-search.focus input {
	left: 45px
}

.breadcrumb {
	padding: 0 40px;
	box-sizing: border-box
}

.breadcrumb>a {
	float: left;
	display: inline-block;
	color: #397b81;
	font-size: 16px;
	cursor: pointer
}

.breadcrumb>span.breadcrumb-next {
	float: left;
	display: inline-block;
	margin: 0 10px;
	width: 12px;
	height: 12px;
	background: url(../img/breadcrumb.png);
	background-size: 100% 100%
}

span.match-icon {
	display: inline-block;
	width: 29px;
	height: 29px;
	margin-bottom: -9px;
	margin-right: 5px;
	background: url(../img/icon_all.png) no-repeat;
	cursor: pointer;
}

span.match-icon {
	background-position: -47px -924px;
}
