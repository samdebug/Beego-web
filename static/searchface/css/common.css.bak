@font-face {
	font-family:transformers;src:url(../css/fonts/transformers.ttf) format('truetype');font-weight:400;font-style:normal
}

* {
	margin: 0;
	padding: 0;
	border: 0;
	outline: 0;
	background: 0 0;
	font: inherit;
	color: inherit
}

body,html {
	margin: 0;
	padding: 0;
	width: 100%;
	text-align: center;
	position: relative;
	box-sizing: border-box;
	font-family: "Microsoft YaHei";
	background: #000;
	overflow-x: hidden
}

select {
	-webkit-appearance: none!important
}

input::-webkit-input-placeholder,textarea::-webkit-input-placeholder {
	color: #53A8AE;
	font-size: 14px
}

input::-webkit-inner-spin-button,input::-webkit-outer-spin-button {
	-webkit-appearance: none!important;
	margin: 0
}

input:-webkit-autofill {
	-webkit-box-shadow: 0 0 0 1000px #1A3034 inset!important;
	-webkit-text-fill-color: rgba(113,245,255,.5)
}[scroll=scroll]::-webkit-scrollbar {
	width: 6px
}[scroll=scroll]::-webkit-scrollbar-track {
	box-shadow: 0
}[scroll=scroll]::-webkit-scrollbar-thumb {
	border-radius: 2px;
	background: #4a9399;
	-webkit-box-shadow: #fff
}[scroll=scroll]::-webkit-scrollbar-thumb:window-inactive {
	background: #4a9399
}[scroll=scroll] {
	scrollbar-arrow-color: #4a9399;
	scrollbar-face-color: #4a9399;
	scrollbar-3dlight-color: #fff;
	scrollbar-highlight-color: #fff;
	scrollbar-shadow-color: #4a9399;
	scrollbar-darkshadow-color: #fff;
	scrollbar-track-color: #fff;
	scrollbar-base-color: #fff
}

